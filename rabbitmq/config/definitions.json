{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "product_name": "RabbitMQ",
  "product_version": "3.12.0",
  "users": [
    {
      "name": "admin",
      "password_hash": "password",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "admin",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "exchanges": [
    {
      "name": "ecommerce.events",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "inventory.critical",
      "vhost": "/",
      "type": "fanout",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "queues": [
    {
      "name": "orders.created",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-dead-letter-exchange": "ecommerce.dlx"
      }
    },
    {
      "name": "notifications.send",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-dead-letter-exchange": "ecommerce.dlx"
      }
    },
    {
      "name": "inventory.critical.notifications",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-dead-letter-exchange": "ecommerce.dlx"
      }
    },
    {
      "name": "inventory.critical.orders",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-dead-letter-exchange": "ecommerce.dlx"
      }
    },
    {
      "name": "inventory.critical.products",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-dead-letter-exchange": "ecommerce.dlx"
      }
    }
  ],
  "bindings": [
    {
      "source": "ecommerce.events",
      "vhost": "/",
      "destination": "orders.created",
      "destination_type": "queue",
      "routing_key": "order.created",
      "arguments": {}
    },
    {
      "source": "ecommerce.events",
      "vhost": "/",
      "destination": "notifications.send",
      "destination_type": "queue",
      "routing_key": "order.created",
      "arguments": {}
    },
    {
      "source": "inventory.critical",
      "vhost": "/",
      "destination": "inventory.critical.notifications",
      "destination_type": "queue",
      "routing_key": "",
      "arguments": {}
    },
    {
      "source": "inventory.critical",
      "vhost": "/",
      "destination": "inventory.critical.orders",
      "destination_type": "queue",
      "routing_key": "",
      "arguments": {}
    },
    {
      "source": "inventory.critical",
      "vhost": "/",
      "destination": "inventory.critical.products",
      "destination_type": "queue",
      "routing_key": "",
      "arguments": {}
    }
  ]
}